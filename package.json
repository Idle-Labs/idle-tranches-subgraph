{
  "name": "idle-tranches",
  "license": "UNLICENSED",
  "scripts": {
    "local-hardhat": "cd idle-tranches && npx hardhat node --hostname 0.0.0.0 --verbose",
    "local-graph": "cd graph-node/docker && docker-compose up",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ idle-finance/idle-tranches",
    "create-local": "graph create --node http://localhost:8020/ idle-finance/idle-tranches",
    "remove-local": "graph remove --node http://localhost:8020/ idle-finance/idle-tranches",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 idle-finance/idle-tranches",
    "deploy-local-e2e": "npm run codegen && npm run build && npm run remove-local && npm run create-local && npm run deploy-local"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.21.1",
    "@graphprotocol/graph-ts": "0.20.0"
  }
}
